{
  "name": "casual/framework",
  "type": "library",
  "keywords": [
    "api",
    "framework",
    "daemon"
  ],
  "license": "MIT",
  "description": "Low performance Service Framework Component for PHP based on Workerman.",
  "authors": [
    {
      "name": "chaz6chez",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "250220719@qq.com",
    "issues": "https://github.com/chaz6chez/casual-frame/issues",
    "source": "https://github.com/chaz6chez/casual-frame"
  },
  "require": {
    "php": ">=7.2",
    "ext-posix": "*",
    "ext-pcntl": "*",
    "symfony/console": "^5.2",
    "workerman/workerman": "^4.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Kernel\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Kernel\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests"
  }
}